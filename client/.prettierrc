{
  "trailingComma": "none",
  "useTabs": true,
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "@/constants/(.*)$",
    "^@/components/(.*)$",
    "^@/types/(.*)$",
    "^@/types$",
    "^@/hooks/(.*)$",
    "^@/hooks$",
    "^@/providers/(.*)$",
    "^@/services/(.*)$",
    "^@/assets/(.*)$",
    "^@/store/(.*)$",
    "^@/utils/(.*)$",
    "^../(?!.*scss)",
    "^./(?!.*scss)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}